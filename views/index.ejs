<html>
    <head>
        <title>Index</title>
    </head>
    <body>
        <div class="messages">
            <% messages.forEach(message => { %>
                <div class="message">
                    <p><%= message.text %></p>
                    <p><%= message.user %></p>
                    <p><%= message.added %></p>
                </div>
            <% }) %>
        </div>
    </body>
</html>
